    <PreferenceCategory android:title="@string/pref_category_title_general" app:iconSpaceReserved="false">
        <EditTextPreference android:digits="-0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ" android:title="@string/pref_title_dfu_ota_service_uuid" android:key="rtk_dfu_ota_service_uuid" android:summary="{v}" android:dialogTitle="@string/pref_title_dfu_ota_service_uuid" android:inputType="text" app:iconSpaceReserved="false" />
